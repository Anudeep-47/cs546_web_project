                        alt=""
                        srcset=""
                      />
                    </div>
                    <div class="col">
                      <p class="fw-bold fs-5">Dr. {{this.name}}, {{this.qualification}}</p>
                      <p class="small">{{this.specialty}}</p>
                      <p class="small text-muted">{{this.location.city}}, {{this.location.state}}</p>
                      <li class="list-inline-item mt-3"><i
                          class="fa fa-star star me-2"
                          aria-hidden="true"
                        ></i>{{this.rating}}</li>
                      <li class="text-right list-inline-item"><a
                          class="nav-link stretched-link"
                          href="/doctor/{{this.doc_id}}"
                        >View Availability</a></li>
                    </div>
                  </div>
                </div>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
        <button
          class="carousel-control-prev"
          type="button"
          data-bs-target="#carouselExampleDark1"
          data-bs-slide="prev"
        >
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </button>
        <button
          class="carousel-control-next"
          type="button"
          data-bs-target="#carouselExampleDark1"
          data-bs-slide="next"
        >
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </button>
      </div>
    </div>
  </div>

