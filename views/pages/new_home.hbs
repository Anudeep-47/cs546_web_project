<div class="container">
  <div class="row">
    <div class="col">
      <div class="card">
        <div class="card-body px-5 py-5">
          <h1 class="home_text">Find Doctors</h1>
          <h1 class="home_text">Who accept your insurance</h1>
          <div class="mt-3">
            {{> searchbar}}
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="row my-5">
    <div>
      <h2>Top Searched Specialties</h2>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">Primary Care</a>
      </div>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">Dentist</a>
      </div>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">OB-GYN</a>
      </div>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">Dermatologist</a>
      </div>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">Psychiatrist</a>
      </div>
    </div>
    <div class="col">
      <div class="card card-body text-center">
        <a class="fs-5 nav-link stretched-link p-0" href="">Eye Doctor</a>
      </div>
    </div>
  </div>

  <div class="row my-5">
    <div class="col">
      <div>
        <h2>
          Top Rated Doctors
        </h2>
      </div>
      <div
        id="carouselExampleDark1"
        class="carousel carousel-dark slide"
        data-bs-ride="carousel"
      >
        <div class="carousel-inner">
          <div class="carousel-item active" data-bs-interval="10000">
            <div class="row">
              {{#each top_rated1}}
              <div class="col">
                <div class="card card-body">
                  <div class="row">
                    <div class="col-4">
                      <img
                        class="img-thumbnail border-secondary rounded-circle"
                        src="/public/img/doctor_male.jpeg"
                        alt=""
                        srcset=""
                      />
                    </div>
                    <div class="col">
                      <p class="fw-bold fs-5">Dr. {{this.name}}, {{this.qualification}}</p>
                      <p class="small">{{this.specialty}}</p>
                      <p class="small text-muted">{{this.location.city}}, {{this.location.state}}</p>
                      <li class="list-inline-item mt-3"><i
                          class="fa fa-star star me-2"
                          aria-hidden="true"
                        ></i>{{this.rating}}</li>
                      <li class="text-right list-inline-item"><a
                          class="nav-link stretched-link"
                          href="/doctor/{{this.doc_id}}"
                        >View Availability</a></li>
                    </div>
                  </div>
                </div>
              </div>
              {{/each}}
            </div>
          </div>
          <div class="carousel-item">
            <div class="row">
              {{#each top_rated2}}
              <div class="col">
                <div class="card card-body">
                  <div class="row">
                    <div class="col-4">
                      <img
                        class="img-thumbnail border-secondary rounded-circle"
